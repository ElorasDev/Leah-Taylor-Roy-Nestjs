{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/api/(.*)",
        "methods": ["GET", "POST", "PATCH", "DELETE"],
        "dest": "src/main.ts"
      }
    ]
  }